#application values
application/x-applix-word
        ext: aw

application/x-gnome-app-info
	ext: desktop

application/x-bzip
	ext: bz2 bz

application/x-troff-man
	regex: (([^0-9]|^[^\.]*)\.([1-9][A-Za-z]*|n)|\.man)$

application/x-troff-man-compressed
	regex: ([^0-9]|^[^\.]*)\.([1-9][a-z]?|n)\.g?[Zz]$

